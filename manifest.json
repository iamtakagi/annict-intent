{
  "name": "annict-intent-tweet",
  "author": "iamtakagi",
  "manifest_version": 2,
  "version": "0.0.0",
  "description": "",
  "permissions": [
    "https://api.annict.com/*",
    "webRequest",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://annict.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "applications": {
    "gecko": {
      "id": "annict-intent-tweet@addons.iamtakagi.net"
    }
  },
  "options_ui": {
    "page": "settings.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'"
}
